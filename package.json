  },
  "devDependencies": {
    "eslint": "3.2.0",
    "eslint-cli": "1.1.0",
    "eslint-config-eslint": "3.0.0",
    "istanbul": "0.4.4",
    "tap-colorize": "1.2.0",
    "tape": "4.6.0",
    "updtr": "0.2.1",
    "watch-cli": "0.2.1"
  },
  "keywords": [
    "d3js",
    "d3-hierarchy"
  ],
  "dependencies": {
    "d3-hierarchy": "1.0.0"
  }
}
