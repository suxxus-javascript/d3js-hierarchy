{
  "name": "d3js-studies",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/suxxus-studies/d3js-hierarchy"
  },
  "author": "suxxus",
  "license": "ISC",
  "description": "module d3js (2D layout algorithms for visualizing hierarchical data)",
  "scripts": {
